<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:Android="http://schemas.android.com/apk/res-auto">
    <!-- Base application theme. -->
    <style name="EditTextStyle">
        <!-- Customize your theme here. -->
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ems">30</item>
        <item name="android:textColor">#0E0E0E</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginRight">10dp</item>

    </style>
    <style name="ButtonStyle">
        <!-- Customize your theme here. -->
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">@drawable/buttons</item>
    </style>
</resources>